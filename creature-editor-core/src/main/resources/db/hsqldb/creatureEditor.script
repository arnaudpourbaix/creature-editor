INSERT INTO ACTION VALUES('SelectWeaponAbility',FALSE,'I:WeaponNumSlots,I:AbilityNum','This action instructs the active creature to select the specified slot, and use the ability in the extended header specified by the ability parameter.')
INSERT INTO ACTION VALUES('SendTrigger',FALSE,'O:Target,I:TriggerNum','This action is used as a form of script communication, in conjunction with the Trigger() trigger. The action has the same range as GiveOrder and affects only one creature at a time. Note that the LastTrigger() object does not get set after receiving a trigger.')
INSERT INTO ACTION VALUES('SetAreaRestFlag',FALSE,'I:CanRest','This action dictates whether resting is allowed in the current area.')
