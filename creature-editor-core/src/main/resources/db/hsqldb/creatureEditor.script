INSERT INTO OPCODE VALUES(300,'Modify collision behavior','This effect allows the targetted creature(s) to push other creatures out of the way while moving. Constant Value should be set to 1. The effect modifies stat 190 to param 2.')
INSERT INTO OPCODE VALUES(301,'Critical Hit Modifier','This effect improves the Critical Hit chances. Critical hits will occur on a roll (on a 1d20) greater or equal to: 20 - ''Statistic Modifier''')
INSERT INTO OPCODE VALUES(302,'Can use any item','The targeted creature(s) can use any item. The Constant Value parameter should be set to 1. The effect modifies stat 191.')
INSERT INTO OPCODE VALUES(303,'Backstab every hit','The targeted creature(s) will backstab with every hit. The Constant Value parameter should be set to 1. The effect modifies stat 192.')
INSERT INTO OPCODE VALUES(304,'Mass raise dead','This effect will raise multiple dead PCs and NPCs.')
INSERT INTO OPCODE VALUES(305,'Off-hand THAC0 modifier','Applies the modifier value specified by the ''Statistic Modifier'' field in the style specified by the ''Type'' field.')
INSERT INTO OPCODE VALUES(306,'Main hand THAC0 modifier','Applies the modifier value specified by the ''Statistic Modifier'' field in the style specified by the ''Type'' field.')
INSERT INTO OPCODE VALUES(307,'Ranger Tracking Ability','Activates the Tracking Ability. A string from tracking.2da is shown, and arrows appear along the screen edge pointing to any creatures within range. The units of the range parameter are unknown. The arrow effect may be hardcoded to spell SPCL922. NB: The file ''tracking.2da'' lists the string reference that shows what information is learned from using this effect for each area.')
INSERT INTO OPCODE VALUES(308,'Immunity to Tracking','Protects the targetted creature(s) from the Tracking arrows effect (has no affect on the area string displayed). The effect modifies stat 195 to the value of param type.')
INSERT INTO OPCODE VALUES(309,'Script: Modify Local Variable','Set both the local variable specified by the resource key to the value specified by the ''Value'' field. ')
INSERT INTO OPCODE VALUES(310,'Immunity to time stop','The targeted creature(s) becomes immune to time-stop. The Constant Value parameter should be set to 1. The effect modifies stat 197.')
INSERT INTO OPCODE VALUES(311,'Wish','This effect casts a wish spell (SPWISH01 - SPWISH25), selected based on the casting creatures Wisdom attribute.')
INSERT INTO OPCODE VALUES(312,'Immunity to sequester','This effect crashes the game.')
INSERT INTO OPCODE VALUES(313,'High-Level Ability Denotation','This effect denotes that the high level ability specified by the resource key has been gained. Used for pre-requisites/count limits.')
INSERT INTO OPCODE VALUES(314,'Golem Stoneskin','Gives the targeted creature(s) a number of protections from physical attacks, as specified by the ''Amount'' field. NB: Do not use this effect in conjunction with the Juggernaut Golem Manual as it will corrupt the use of that item.')
INSERT INTO OPCODE VALUES(315,'Remove animation','This effect will remove the animation of the targeted creature(s). The Constant Value parameter should be set to a non-zero value. The effect modifies stat 201.')
INSERT INTO OPCODE VALUES(316,'Rest','This effect will refresh spells and remove any fatigue.')
INSERT INTO OPCODE VALUES(317,'Haste 2','Applies the Haste effect, in the style specified by the ''Type'' value.')
INSERT INTO OPCODE VALUES(318,'Unknown','')
INSERT INTO OPCODE VALUES(319,'Restrict item','This effect restricts the item to creature(s) matching the object identifiers. The string to display when an invalid creature attempts to use the item is held in 0x2c.')
INSERT INTO OPCODE VALUES(320,'Change weather','Changes the weather of the current area to the weather specified by ''Weather''.')
INSERT INTO OPCODE VALUES(321,'Remove effects by resource','Removes effects from the resource specified by the resouce key, in the style specified by the ''Type'' field.')
