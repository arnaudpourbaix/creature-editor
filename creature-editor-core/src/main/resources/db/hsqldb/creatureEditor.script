INSERT INTO ACTION VALUES('SetGabber',FALSE,'O:Object','This action updates various tokens based on the specified object.')
INSERT INTO ACTION VALUES('SetGlobal',FALSE,'S:Name,S:Area,I:Value','This action sets a variable (specified by name) in the scope (specified by area) to the value (specified by value). See the variable type appendix for details on variables.')
