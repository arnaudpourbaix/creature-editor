INSERT INTO ACTION VALUES('GiveItemCreate',FALSE,'S:ResRef,O:Object,I:Usage1,I:Usage2,I:Usage3','This action creates the item specified by the resref parameter on the creature specified by the object parameter, with quantity/charges controlled by the usage parameters.')
