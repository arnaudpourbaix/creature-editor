INSERT INTO "TRIGGER" VALUES('AreaCheck',FALSE,'S:ResRef','Returns true only if the active CRE is in the area specified.')
INSERT INTO "TRIGGER" VALUES('AreaCheckObject',FALSE,'S:ResRef,O:Object','Returns true if the specified object is in the area specified.')
INSERT INTO "TRIGGER" VALUES('AreaType',FALSE,'I:AREATYPE','Returns true only if the area in which the active CRE is, has the specified type flag set. Note that the value is OR''d. This trigger can cause a crash if it evaluates to true and is used as the first trigger in a dead NPCs dream script.')
INSERT INTO "TRIGGER" VALUES('Assign',TRUE,'S:Statement,I:Type,I:Local','Assigns a value determined by Statement of the type Type from ARGTYPE.IDS (INT integer, or STR string) to a local trigger block variable. The general form of Statement is "prefix[params]". This trigger does not evaluate and does not count as a trigger in an OR() block.')
INSERT INTO "TRIGGER" VALUES('AttackedBy',FALSE,'O:Object,I:Style','Returns true only if the active CRE was attacked in the style specified (not necessarily hit) or had an offensive spell cast on it by the specified object in the last script round. The style parameter is non functional - this trigger is triggered by any attack style.')
INSERT INTO "TRIGGER" VALUES('BecameVisible',FALSE,NULL,'Returns true only if the active CRE turned visible in the last script round.')
INSERT INTO "TRIGGER" VALUES('BouncingSpellLevel',TRUE,'O:Object,I:Level','Returns true if the target creature specified by Object is bouncing spells of power Level. This returns true for both Bounce Spell Level (199) and Decrementing Bounce Spells (200) effects.')
INSERT INTO "TRIGGER" VALUES('BreakingPoint',FALSE,NULL,'Checks the current creatures happiness value. Note that this trigger is only evaluated when the happiness value is checked (at all other times this trigger returns false).')
INSERT INTO "TRIGGER" VALUES('CalanderDay',FALSE,'I:Day','Returns true only if the current day is the day specified.')
INSERT INTO "TRIGGER" VALUES('CalanderDayGT',FALSE,'I:Day','Returns true only if the current day is after the day specified.')
INSERT INTO "TRIGGER" VALUES('CalanderDayLT',FALSE,'I:Day','Returns true only if the current day is before the day specified.')
INSERT INTO "TRIGGER" VALUES('CharName',FALSE,'S:Name,O:Object','Returns true only if the specified object has the character name specified.')
INSERT INTO "TRIGGER" VALUES('CheckStat',FALSE,'O:Object,I:Value,I:StatNum','Returns true only if the specified object has the statistic in the 3rd parameter at the value of the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('CheckStatBAND',TRUE,'O:Object,I:Value,I:StatNum','Returns true only if the specified object has the statistic in the 3rd parameter produces a non-zero value when executing a bitwise AND with the value of the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('CheckStatGT',FALSE,'O:Object,I:Value,I:StatNum','Returns true only if the specified object has the statistic in the 3rd parameter greater than the value of the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('CheckStatLT',FALSE,'O:Object,I:Value,I:StatNum','Returns true only if the specified object has the statistic in the 3rd parameter less than the value of the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('Class',FALSE,'O:Object,I:Class','Returns true only if the Class of the specified object matches that in the second parameter.')
INSERT INTO "TRIGGER" VALUES('Clicked',FALSE,'O:Object','Only for trigger regions. Returns true if the specified object clicked on the trigger region running this script.')
INSERT INTO "TRIGGER" VALUES('Closed',FALSE,'O:Object','Only for door scripts. Returns true only if the specified object closed the active door in the last script round.')
INSERT INTO "TRIGGER" VALUES('CombatCounter',FALSE,'I:Number','Returns true only if the combat counter (counts down from 150 after each attack) is of the value specified. CombatCounter(0) returns true only if there is no combat in the active area at the moment. CombatCounter can give false results after dialog has been completed or after a Hide in Shadows chck has been failed.')
INSERT INTO "TRIGGER" VALUES('CombatCounterGT',FALSE,'I:Number','Returns true only if the combat counter (counts down from 150 after each attack) is greater than the value specified. CombatCounterGT(0) returns true if there is any combat going on in the active area.')
INSERT INTO "TRIGGER" VALUES('CombatCounterLT',FALSE,'I:Number','Returns true only if the combat counter (counts down from 150 after each attack) is less than the value specified.')
INSERT INTO "TRIGGER" VALUES('Contains',FALSE,'S:ResRef,O:Object','Returns true only if the item specified in parameter 1 is in the container specified in parameter 2.')
INSERT INTO "TRIGGER" VALUES('DamageTaken',FALSE,'I:Amount','Returns true if the total damage taken by the active CRE is of the amount specified. Think of this as the reverse of testing for HP. See HP.')
INSERT INTO "TRIGGER" VALUES('DamageTakenGT',FALSE,'I:Amount','See above and HPGT. This trigger may not act correctly until the active creature has dropped below their maximum base HP (i.e. without CON bonus taken into account).')
INSERT INTO "TRIGGER" VALUES('DamageTakenLT',FALSE,'I:Amount','See above and HPLT.')
INSERT INTO "TRIGGER" VALUES('Dead',FALSE,'S:Name','Returns only true if the creature with the specified script name has its death variable set to 1. Not every form of death sets this, but most do. So it''s an almost complete test for death. The creature must have existed for this to be true. Note that SPRITE_IS_DEAD variables are not set if the creaure is killed by a neutral creature.')
INSERT INTO "TRIGGER" VALUES('Delay',FALSE,'I:Delay','Delays the next check of the block of triggers where this trigger is, by the number of seconds specified. This value is not stored when the game is saved.')
INSERT INTO "TRIGGER" VALUES('Detect',FALSE,'O:Object','Returns true if the the specified object is detected by the active CRE in any way (hearing or sight). Neither Move Silently and Hide in Shadows prevent creatures being detected via Detect(). Detect ignores Protection from Creature type effects for static objects.')
INSERT INTO "TRIGGER" VALUES('Detected',FALSE,'O:Object','Only for trap scripts. Returns true only if the specified object detected this trap in the last script round.')
INSERT INTO "TRIGGER" VALUES('Die',FALSE,NULL,'Returns true if the active CRE has died in the last script round. When a block returns true to this trigger, this will be the final block executed in the script, unless it is Continue''d')
INSERT INTO "TRIGGER" VALUES('Died',FALSE,'O:Object','Returns true only if the specifed object died in the last script round.')
INSERT INTO "TRIGGER" VALUES('Difficulty',FALSE,'I:Amount','Returns true only if the game difficulty setting is of the level specified.')
INSERT INTO "TRIGGER" VALUES('DifficultyGT',FALSE,'I:Amount','Returns true only if the game difficulty setting is greater than the level specified.')
INSERT INTO "TRIGGER" VALUES('DifficultyLT',FALSE,'I:Amount','Returns true only if the game difficulty setting is less than the level specified.')
INSERT INTO "TRIGGER" VALUES('DisarmFailed',FALSE,'O:Object','Not used in any existing scripts.')
INSERT INTO "TRIGGER" VALUES('Disarmed',FALSE,'O:Object','Only for trap/trigger region scripts. Returns true only if the specified object disarmed this trap in the last script round.')
INSERT INTO "TRIGGER" VALUES('E',TRUE,'I:Num1,I:Num2','Compares "Num1" to "Num2", where E is equals, GT is greater than, and LT is less than. To make use of these triggers, the Eval() trigger should be used prior to this trigger.')
INSERT INTO "TRIGGER" VALUES('Entered',FALSE,'O:Object','Only for trigger regions in areas. Returns true only if the specified object entered the trigger region in the last script round.')
INSERT INTO "TRIGGER" VALUES('Eval',TRUE,'S:Expression,I:Type,I:Loc','Overwrites the (Loc)th argument of type Type from ARGTYPE.IDS (INT integer, or STR string) in the next trigger with the value returned by Expression. This trigger does not evaluate and does not count as a trigger in an OR() block. This trigger does not overwrite values of the Assign(), NextTriggerObject() and OR() triggers. The NextTriggerObject() trigger ignores this trigger.')
INSERT INTO "TRIGGER" VALUES('Exists',FALSE,'O:Object','Returns true only if the specified object exists in the current area (note that dead creatures can still be counted as existing).')
INSERT INTO "TRIGGER" VALUES('FallenPaladin',FALSE,'O:Object','Returns true only if the specified object is a fallen ranger.')
INSERT INTO "TRIGGER" VALUES('FallenRanger',FALSE,'O:Object','Returns true only if the specified object is a fallen ranger.')
INSERT INTO "TRIGGER" VALUES('False',FALSE,NULL,'Never returns true, i.e. is always false. A block of triggers containing this will never return true regardless of the other triggers in the block. The corresponding actions will never take place.')
INSERT INTO "TRIGGER" VALUES('GT',TRUE,'I:Num1,I:Num2','Compares "Num1" to "Num2", where E is equals, GT is greater than, and LT is less than. To make use of these triggers, the Eval() trigger should be used prior to this trigger.')
INSERT INTO "TRIGGER" VALUES('Gender',FALSE,'O:Object,I:Gender','Returns true only if the gender of the specified object is that given in the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('General',FALSE,'O:Object,I:General','Returns true only if the General category of the specified object matches that in the second parameter.')
INSERT INTO "TRIGGER" VALUES('Global',FALSE,'S:Name,S:Area,I:Value','Returns true only if the variable with name 1st parameter of type 2nd parameter has value 3rd parameter.')
INSERT INTO "TRIGGER" VALUES('GlobalGT',FALSE,'S:Name,S:Area,I:Value','See Global(S:Name,S:Area,I:Value) except the variable must be greater than the value specified to be true.')
INSERT INTO "TRIGGER" VALUES('GlobalLT',FALSE,'S:Name,S:Area,I:Value','As above except for less than.')
INSERT INTO "TRIGGER" VALUES('GlobalTimerExpired',FALSE,'S:Name,S:Area','Returns true only if the timer with the name specified and of the type in the 2nd parameter has run and expired.')
INSERT INTO "TRIGGER" VALUES('GlobalTimerNotExpired',FALSE,'S:Name,S:Area','Returns true only if the timer with the name specified and of the type in the 2nd parameter is still running. Note that if we use !GlobalTimerNotExpired(S:Name,S:Area) this will return true if the timer has never been set OR if it has already expired- very useful...most useful of all the GlobalTimer triggers :) .')
INSERT INTO "TRIGGER" VALUES('GlobalsEqual',FALSE,'S:Name1,S:Name2','Returns true only if the 2 global variables specified have equal values.')
INSERT INTO "TRIGGER" VALUES('GlobalsGT',FALSE,'S:Name1,S:Name2','Returns true only if the 1st global variable has a value greater than the 2nd one.')
INSERT INTO "TRIGGER" VALUES('GlobalsLT',FALSE,'S:Name1,S:Name2','Returns true only if the 1st global variable has a value less than the 2nd one.')
INSERT INTO "TRIGGER" VALUES('HP',FALSE,'O:Object,I:Hit Points','Returns true only if the current hitpoints of the specified object are equal to the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('HPGT',FALSE,'O:Object,I:Hit Points','Returns true only if the current hitpoints of the specified object are greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('HPLT',FALSE,'O:Object,I:Hit Points','Returns true only if the current hitpoints of the specified object are less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('HPLost',TRUE,'O:Object,I:Hit Points','Compares the absolute number of hit points short of maximum hit points on the target creature specified by Object to Amount.')
INSERT INTO "TRIGGER" VALUES('HPLostGT',TRUE,'O:Object,I:Hit Points','Compares the absolute number of hit points short of maximum hit points on the target creature specified by Object to Amount.')
INSERT INTO "TRIGGER" VALUES('HPLostLT',TRUE,'O:Object,I:Hit Points','Compares the absolute number of hit points short of maximum hit points on the target creature specified by Object to Amount.')
INSERT INTO "TRIGGER" VALUES('HPPercent',FALSE,'O:Object,I:Hit Points','See HP(O:Object,I:Hit Points) except this is for a percentage.')
INSERT INTO "TRIGGER" VALUES('HPPercentGT',FALSE,'O:Object,I:Hit Points','See HPGT(O:Object,I:Hit Points) except this is for a percentage.')
INSERT INTO "TRIGGER" VALUES('HPPercentLT',FALSE,'O:Object,I:Hit Points','See HPLT(O:Object,I:Hit Points) except this is for a percentage.')
INSERT INTO "TRIGGER" VALUES('Happiness',FALSE,'O:Object,I:Amount','Returns true only if the specified object has the specified happiness value.')
INSERT INTO "TRIGGER" VALUES('HappinessGT',FALSE,'O:Object,I:Amount','Returns true only if the specified object has greater than the specified happiness value.')
INSERT INTO "TRIGGER" VALUES('HappinessLT',FALSE,'O:Object,I:Amount','Returns true only if the specified object has less than the specified happiness value.')
INSERT INTO "TRIGGER" VALUES('HasBounceEffects',FALSE,'O:Object','Returns true if any of the specified object is currently affected by any of the listed effects: Bounce Projectile, Bounce Opcode, Bounce Spell Level, Bounce Spell School, Bounce Secondary Type, Bounce Specified School, Bounce School Decrement, Bounce Secondary Type Decrement')
INSERT INTO "TRIGGER" VALUES('HasImmunityEffects',FALSE,'O:Object','Returns true if any of the specified object is currently affected by any of the listed effects: Protection from projectiles, Protection from effects, Protection from portrait icon, Protection from display string, Protection from visual effect, Immunity to spell level, Decrementing spell level immunity, Immunity to primary type (school), Immunity to secondary type, Decrementing immunity to primary type, Decrementing immunity to secondary type')
INSERT INTO "TRIGGER" VALUES('HasItem',FALSE,'S:ResRef,O:Object','Returns true only if the specified object has the specified item in its inventory. This trigger also checks with container items (e.g. Bags of Holding).')
INSERT INTO "TRIGGER" VALUES('HasItemEquiped',FALSE,'S:ResRef,O:Object','Returns true if the specified object has the specified item in one of its quicklots if it is a weapon, or has it equipped if it is a piece of armour of an amulet etc. This trigger does not work for Melf''s Minute Meteors.')
INSERT INTO "TRIGGER" VALUES('HasItemEquipedReal',FALSE,'S:ResRef,O:Object','Returns true only if the specified object has the specified item equipped and it is in use. This trigger does not work for Melf''s Minute Meteors.')
INSERT INTO "TRIGGER" VALUES('HasItemSlot',FALSE,'O:Object,I:Slot','Returns true only if the specified object has an item in the specified slot. This trigger does not work for Melf''s Minute Meteors.')
INSERT INTO "TRIGGER" VALUES('HasWeaponEquiped',FALSE,'O:Object','Returns true only if the specified object has a weapon in a quickslot.')
INSERT INTO "TRIGGER" VALUES('HaveAnySpells',FALSE,NULL,'Returns true if the active CRE has at least one spell memorised.')
INSERT INTO "TRIGGER" VALUES('HaveSpell',FALSE,'I:Spell','Returns true only if the active CRE has the specified spell memorised.')
INSERT INTO "TRIGGER" VALUES('HaveSpellParty',FALSE,'I:Spell','Returns true if any of the party members have the specified spell memorised.')
INSERT INTO "TRIGGER" VALUES('HaveSpellRES',FALSE,'S:Spell','Returns true if the active CRE has the specified (by the string parameter) spell memorised.')
INSERT INTO "TRIGGER" VALUES('Heard',FALSE,'O:Object,I:SHOUTIDS','Returns true only if the active CRE was within 30 feet of the specified object and the specified object shouted the specified number (which does not have to be in SHOUTIDS.ids) in the last script round. If the object is specified as a death variable, the trigger will only return true if the corresponding object shouting also has an Enemy-Ally flag of NEUTRAL.')
INSERT INTO "TRIGGER" VALUES('Help',FALSE,'O:Object','Returns true if the specified object shouted for Help() in the two script rounds. Help() has a range of approximately 40.')
INSERT INTO "TRIGGER" VALUES('HitBy',FALSE,'O:Object,I:DameType','Returns true only if the active CRE was hit by the specifed object by the specified damage type in the last script round. If the damage type is CRUSHING or 0 then this will return true for ANY damage type. !HitBy returns true when the script is first activated (e.g. initial area load) and when hit by any damage type. ')
INSERT INTO "TRIGGER" VALUES('HotKey',FALSE,'I:Key','Returns true only if the specified key was pressed in the last script round. Hotkeys defined in keymap.ini take precedence over hotkeys expected by this trigger.')
INSERT INTO "TRIGGER" VALUES('IfValidForPartyDialogue',FALSE,'O:Object','Returns true only if that specifed party member can take part in party dialogue.')
INSERT INTO "TRIGGER" VALUES('ImmuneToSpellLevel',TRUE,'O:Object,I:Level','Returns true if the target creature specified by Object is protected from spells of power Level. This returns true for both Protection from Spell Levels (102) and Decrementing Spell Immunity (201) effects.')
INSERT INTO "TRIGGER" VALUES('InActiveArea',FALSE,'O:Object','Returns true only if specified object is in the active area. The active area is that in which player 1 is.')
INSERT INTO "TRIGGER" VALUES('InLine',FALSE,'O:Object','Returns true if the active creature can see the target creature.')
INSERT INTO "TRIGGER" VALUES('InMyArea',FALSE,'O:Object','Returns true only if the specified object is in the same area as the active CRE.')
INSERT INTO "TRIGGER" VALUES('InMyGroup',FALSE,'O:Object','Returns true only if the specifics value of the specified object is the same as that of the active CRE.')
INSERT INTO "TRIGGER" VALUES('InParty',FALSE,'O:Object','Returns true only if the specifed object is in the player''s party.')
INSERT INTO "TRIGGER" VALUES('InPartyAllowDead',FALSE,'O:Object','Returns true if the specified object which can be dead or alive is in the party.')
INSERT INTO "TRIGGER" VALUES('InPartySlot',FALSE,'O:Object,I:Slot','Returns true only if the specified object is in the party and in the slot specified (slots are 0-5). Party slots are based on join order')
INSERT INTO "TRIGGER" VALUES('InWeaponRange',FALSE,'O:Object','Returns true only if the specified object is within the range of the active CRE''s currently equipped weapon.')
INSERT INTO "TRIGGER" VALUES('InteractingWith',FALSE,'O:Object','Returns true only if the active CRE is interacting (dialogue?) with the specified object.')
INSERT INTO "TRIGGER" VALUES('InventoryFull',FALSE,'O:Object','Returns true only if the specified object can carry no more items.')
INSERT INTO "TRIGGER" VALUES('IsActive',FALSE,'O:Object','Returns true if the specified creature is active and false if it is deactivated. A creature will continue to execute script blocks even while deactivated - this trigger can be used to restrict this behaviour.')
INSERT INTO "TRIGGER" VALUES('IsGabber',FALSE,'O:Object','Returns true only if the specified object is the current speaker (in a dialog).')
INSERT INTO "TRIGGER" VALUES('IsOverMe',FALSE,'O:Object','Only for trigger regions. Returns true only if the specified object is over the trigger running the script.')
INSERT INTO "TRIGGER" VALUES('Joins',FALSE,'O:Object','Returns true if the specified object has joined the party in the last script round. This trigger is only sent to player characters.')
INSERT INTO "TRIGGER" VALUES('Killed',FALSE,'O:Object','Returns true if the active CRE killed the specified object in the last script round.')
INSERT INTO "TRIGGER" VALUES('Kit',FALSE,'O:Object,I:Kit','Returns true only if the specified object is of the kit specified. A creature''s assigned kit is stored as a dword, however the Kit() trigger only checks the upper word. This, in conjunction with various incorrect values in the game cre files, and an incorrect kits.ids file, means the Kit() trigger can often fail. For optimal usage, the default kit.ids file should be replaced with the updated one listed in the BG2: ToB ids page.')
INSERT INTO "TRIGGER" VALUES('LOS',FALSE,'O:Object,I:Range','Returns true only if the object specified is in the line of sight of the active CRE and within the given range. This seems to be a combination of the Range and See triggers (see below). Range seems limited to the default visual range (30).')
INSERT INTO "TRIGGER" VALUES('LT',TRUE,'I:Num1,I:Num2','Compares "Num1" to "Num2", where E is equals, GT is greater than, and LT is less than. To make use of these triggers, the Eval() trigger should be used prior to this trigger.')
INSERT INTO "TRIGGER" VALUES('Leaves',FALSE,'O:Object','Returns true if the specified object has left the party in the last script round. This trigger is only sent to player characters.')
INSERT INTO "TRIGGER" VALUES('Level',FALSE,'O:Object,I:Level','Returns true only if the experience level of the specified object equals the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('LevelGT',FALSE,'O:Object,I:Level','Returns true only if the experience level of the specified object is greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('LevelLT',FALSE,'O:Object,I:Level','Returns true only if the experience level of the specified object is less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('LevelParty',FALSE,'I:Num','Returns true only if the level of the highest-levelled party member is equal to the number specified.')
INSERT INTO "TRIGGER" VALUES('LevelPartyGT',FALSE,'I:Num','Returns true only if the level of the highest-levelled party member is greater than the number specified.')
INSERT INTO "TRIGGER" VALUES('LevelPartyLT',FALSE,'I:Num','Returns true only if the level of the highest-levelled party member is less than the number specified.')
INSERT INTO "TRIGGER" VALUES('LocalsEqual',FALSE,'S:Name1,S:Name2','Returns true only if the 2 local variables specified have equal values.')
INSERT INTO "TRIGGER" VALUES('LocalsGT',FALSE,'S:Name1,S:Name2','Returns true only if the 1st local variable has a value less than the 2nd one.')
INSERT INTO "TRIGGER" VALUES('LocalsLT',FALSE,'S:Name1,S:Name2','Returns true only if the 1st local variable has a value less than the 2nd one.')
INSERT INTO "TRIGGER" VALUES('ModalState',FALSE,'I:State','Returns true only if the active CRE is in the state/mode specified. e.g. detecting traps.')
INSERT INTO "TRIGGER" VALUES('Morale',FALSE,'O:Object,I:Morale','Returns true only if the morale of the specified object is equal to the second parameter.')
INSERT INTO "TRIGGER" VALUES('MoraleGT',FALSE,'O:Object,I:Morale','Returns true only if the morale of the specified object is greater than thesecond parameter.')
INSERT INTO "TRIGGER" VALUES('MoraleLT',FALSE,'O:Object,I:Morale','Returns true only if the morale of the specified object is less than the second parameter.')
INSERT INTO "TRIGGER" VALUES('MovementRate',TRUE,'O:Object,I:Value','Compares the animation movement rate of the target creature specified by Object to Value. This is not affected by slow or haste, but is affected if the Object is entangled, webbed, etc.')
INSERT INTO "TRIGGER" VALUES('MovementRateGT',TRUE,'O:Object,I:Value','Compares the animation movement rate of the target creature specified by Object to Value. This is not affected by slow or haste, but is affected if the Object is entangled, webbed, etc.')
INSERT INTO "TRIGGER" VALUES('MovementRateLT',TRUE,'O:Object,I:Value','Compares the animation movement rate of the target creature specified by Object to Value. This is not affected by slow or haste, but is affected if the Object is entangled, webbed, etc.')
INSERT INTO "TRIGGER" VALUES('Name',FALSE,'S:Name,O:Object','Returns true only if the object in the 2nd parameter has the scriptname specified.')
INSERT INTO "TRIGGER" VALUES('NextTriggerObject',TRUE,'O:Object','Defines the object that the next trigger will be evaluated in reference to. This trigger does not evaluate and does not count as a trigger in an OR() block. This trigger ignores the Eval() trigger when finding the next trigger to evaluate the object for. If the object cannot be found, the next trigger will evaluate to false.')
INSERT INTO "TRIGGER" VALUES('NotStateCheck',FALSE,'O:Object,I:State','Exact opposite of above.')
INSERT INTO "TRIGGER" VALUES('NumBouncingSpellLevel',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell bounces remaining on the target creature specified by Object at the power Level to Amount. If Object has the Bounce Spell Level (199) opcode, then the number of spell bounces is unsigned 0xFFFFFFFF.')
INSERT INTO "TRIGGER" VALUES('NumBouncingSpellLevelGT',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell bounces remaining on the target creature specified by Object at the power Level to Amount. If Object has the Bounce Spell Level (199) opcode, then the number of spell bounces is unsigned 0xFFFFFFFF.')
INSERT INTO "TRIGGER" VALUES('NumBouncingSpellLevelLT',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell bounces remaining on the target creature specified by Object at the power Level to Amount. If Object has the Bounce Spell Level (199) opcode, then the number of spell bounces is unsigned 0xFFFFFFFF.')
INSERT INTO "TRIGGER" VALUES('NumCreature',FALSE,'O:Object,I:Number','Returns true only if the number of creatures of the type specified in sight of the active CRE are equal to the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumCreatureGT',FALSE,'O:Object,I:Number','Returns true only if the number of creatures of the type specified in sight of the active CRE are greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumCreatureLT',FALSE,'O:Object,I:Number','Returns true only if the number of creatures of the type specified in sight of the active CRE are less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumCreatureVsParty',FALSE,'O:Object,I:Number','Returns true if the number of hostile creatures of the type specified in the 1st parameter that are currently in fighting range of the party, minus the number of party members, is equal to the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumCreatureVsPartyGT',FALSE,'O:Object,I:Number','Returns true if the number of hostile creatures of the type specified in the 1st parameter that are currently in fighting range of the party, minus the number of party members, is greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumCreatureVsPartyLT',FALSE,'O:Object,I:Number','Returns true if the number of hostile creatures of the type specified in the 1st parameter that are currently in fighting range of the party, minus the number of party members, is less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumDead',FALSE,'S:Name,I:Num','Returns true only if the number of creatures with script name "Name" that have been killed is equal to the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumDeadGT',FALSE,'S:Name,I:Num','Returns true only if the number of creatures with script name "Name" that have been killed is greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumDeadLT',FALSE,'S:Name,I:Num','Returns true only if the number of creatures with script name "Name" that have been killed is less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumImmuneToSpellLevel',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell protections remaining on the target creature specified by Object at the power Level to Amount. If Object has the Protection from Spell Levels (102) opcode, then the number of spell protections is unsigned 0xFFFFFFFF.')
INSERT INTO "TRIGGER" VALUES('NumImmuneToSpellLevelGT',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell protections remaining on the target creature specified by Object at the power Level to Amount. If Object has the Protection from Spell Levels (102) opcode, then the number of spell protections is unsigned 0xFFFFFFFF.')
INSERT INTO "TRIGGER" VALUES('NumImmuneToSpellLevelLT',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell protections remaining on the target creature specified by Object at the power Level to Amount. If Object has the Protection from Spell Levels (102) opcode, then the number of spell protections is unsigned 0xFFFFFFFF.')
INSERT INTO "TRIGGER" VALUES('NumInParty',FALSE,'I:Num','Returns true only if the number of party members (dead ones also count) is equal to the number specified.')
INSERT INTO "TRIGGER" VALUES('NumInPartyAlive',FALSE,'I:Num','Returns true only if the number of party members alive is equal to the number specified.')
INSERT INTO "TRIGGER" VALUES('NumInPartyAliveGT',FALSE,'I:Num','Returns true only if the number of party members alive is greater than the number specified.')
INSERT INTO "TRIGGER" VALUES('NumInPartyAliveLT',FALSE,'I:Num','Returns true only if the number of party members alive is less than the number specified.')
INSERT INTO "TRIGGER" VALUES('NumInPartyGT',FALSE,'I:Num','Returns true only if the number of party members (dead ones also count) is greater than the number specified.')
INSERT INTO "TRIGGER" VALUES('NumInPartyLT',FALSE,'I:Num','Returns true only if the number of party members (dead ones also count) is less than the number specified.')
INSERT INTO "TRIGGER" VALUES('NumItems',FALSE,'S:ResRef,O:Object,I:Num','Returns true only if the specified object has the number of items in the 3rd parameter of the type specified in the 1st parameter in its inventory.')
INSERT INTO "TRIGGER" VALUES('NumItemsGT',FALSE,'S:ResRef,O:Object,I:Num','Returns true only if the specified object has more than the number of items in the 3rd parameter of the type specified in the 1st parameter in its inventory.')
INSERT INTO "TRIGGER" VALUES('NumItemsLT',FALSE,'S:ResRef,O:Object,I:Num','Returns true only if the specified object has fewer than the number of items in the 3rd parameter of the type specified in the 1st parameter in its inventory.')
INSERT INTO "TRIGGER" VALUES('NumItemsParty',FALSE,'S:ResRef,I:Num','Returns true only if the party has a total number of items of the type specified equal to the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumItemsPartyGT',FALSE,'S:ResRef,I:Num','Returns true only if the party has a total number of items of the type specified greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumItemsPartyLT',FALSE,'S:ResRef,I:Num','Returns true only if the party has a total number of items of the type specified less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumMirrorImages',TRUE,'O:Object,I:Value','Compares the number of mirror images present on the target creature specified by Object to Value')
INSERT INTO "TRIGGER" VALUES('NumMirrorImagesGT',TRUE,'O:Object,I:Value','Compares the number of mirror images present on the target creature specified by Object to Value')
INSERT INTO "TRIGGER" VALUES('NumMirrorImagesLT',TRUE,'O:Object,I:Value','Compares the number of mirror images present on the target creature specified by Object to Value')
INSERT INTO "TRIGGER" VALUES('NumTimesInteracted',FALSE,'I:NPC,I:Num','Returns true only if the specified NPC has interacted with the party a number of times equal to the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumTimesInteractedGT',FALSE,'I:NPC,I:Num','Returns true only if the specified NPC has interacted with the party a number of times greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumTimesInteractedLT',FALSE,'I:NPC,I:Num','Returns true only if the specified NPC has interacted with the party a number of times less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('NumTimesTalkedTo',FALSE,'I:Num','Returns true only if the player''s party has spoken to the active CRE the exact number of times specified. NumTimesTalkedTo seems to increment when a PCinitiates conversion with an NPC, or an NPC initiates conversation with a PC. NumTimesTalkedTo does not seem to increment for force-talks, interactions, interjections and self-talking.')
INSERT INTO "TRIGGER" VALUES('NumTimesTalkedToGT',FALSE,'I:Num','Returns true only if the player''s party has spoken to the active CRE more than the number of times specified.')
INSERT INTO "TRIGGER" VALUES('NumTimesTalkedToLT',FALSE,'I:Num','Returns true only if the player''s party has spoken to the active CRE less than the number of times specified.')
INSERT INTO "TRIGGER" VALUES('NumTrappingSpellLevel',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell traps remaining on the target creature specified by Object at the power Level to Amount.')
INSERT INTO "TRIGGER" VALUES('NumTrappingSpellLevelGT',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell traps remaining on the target creature specified by Object at the power Level to Amount.')
INSERT INTO "TRIGGER" VALUES('NumTrappingSpellLevelLT',TRUE,'O:Object,I:Level,I:Amount','Compares the number of spell traps remaining on the target creature specified by Object at the power Level to Amount.')
INSERT INTO "TRIGGER" VALUES('NumberOfTimesTalkedTo',FALSE,'I:Num','Returns true only if the number of times the party has spoken to this creature is equal to the number specified.')
INSERT INTO "TRIGGER" VALUES('OR',FALSE,'I:OrCount','Returns true if any of the next ''OrCount'' triggers returns true.')
INSERT INTO "TRIGGER" VALUES('ObjectActionListEmpty',FALSE,'O:Object','Returns true only if the specified object has no actions waiting to be performed.')
INSERT INTO "TRIGGER" VALUES('OnCreation',FALSE,NULL,'Returns true if the script is processed for the first time this session, e.g. when a creature is created (for CRE scripts) or when the player enters an area (for ARE scripts).')
INSERT INTO "TRIGGER" VALUES('OnScreen',FALSE,'O:Object','Returns true if the specified object is on the screen.')
INSERT INTO "TRIGGER" VALUES('OpenState',FALSE,'O:Object,I:Open','Returns true only if the open state of the specified door matches the state specified in the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('Opened',FALSE,'O:Object','Only for door scripts. Returns true only if the specified object opened the active door in the last script round.')
INSERT INTO "TRIGGER" VALUES('OriginalClass',TRUE,'O:Object,I:Class','Returns true if the target creature specified by Object is dual-classed and the original class matches Class.')
INSERT INTO "TRIGGER" VALUES('OutOfAmmo',FALSE,NULL,'Seems to be broken. Returns true only if the active CRE has no ammunition for the current ranged weapon.')
INSERT INTO "TRIGGER" VALUES('PartyGold',FALSE,'I:Amount','Returns true only if the player''s party has the amount of gold specified in the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('PartyGoldGT',FALSE,'I:Amount','Returns true only if the player''s party has more gold than specified in the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('PartyGoldLT',FALSE,'I:Amount','Returns true only if the player''s party has less gold than specified in the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('PartyHasItem',FALSE,'S:Item','Returns true if any of the party members have the specified item in their inventory. This trigger also checks with container items (e.g. Bags of Holding).')
INSERT INTO "TRIGGER" VALUES('PartyHasItemIdentified',FALSE,'S:ResRef','Returns true only if the party has the item specified and it is identified.')
INSERT INTO "TRIGGER" VALUES('PartyMemberDied',FALSE,'O:Object','Returns true only if the party member specified died in the last script round.')
INSERT INTO "TRIGGER" VALUES('PartyRested',FALSE,NULL,'Returns true if the party has just finished resting.')
INSERT INTO "TRIGGER" VALUES('PersonalSpaceDistance',FALSE,'O:Object,I:Range','Returns true if the specified object exactly as far away from the active CRE as the 2nd parameter specifies.')
INSERT INTO "TRIGGER" VALUES('PickLockFailed',FALSE,'O:Object','Not used in any existing scripts.')
INSERT INTO "TRIGGER" VALUES('PickPocketFailed',TRUE,'O:Object','Returns false when a pickpocket attempt fails. The standard AttackedBy() trigger is also sent as per vanilla behaviour.')
INSERT INTO "TRIGGER" VALUES('Race',FALSE,'O:Object,I:Race','Returns true only if the Race of the specified object is the same as that specified by the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('RandomNum',FALSE,'I:Range,I:Value','Generates a random number between 1 and Range. Returns true only if the random number equals the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('RandomNumGT',FALSE,'I:Range,I:Value','As above except returns true only if the random number is greater than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('RandomNumLT',FALSE,'I:Range,I:Value','As above except returns true only if the random number is less than the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('Range',FALSE,'O:Object,I:Range','Returns true only if the specified object is within distance given (in feet) of the active CRE. Range seems limited to the default visual range (30), and does not bypass objects. Range is affected by foot circle size (e.g. the minimum range for a huge foot circle creature (Dragon) is 8). Melee range is 4.')
INSERT INTO "TRIGGER" VALUES('Reaction',FALSE,'O:Object,I:Value','Returns true only if the reaction of the object specified (on the friendly-hostile scale) to the player was as specified. Reaction = 10 + rmodchr + rmodrep (see rmodchr.2da and rmodrep.2da).')
INSERT INTO "TRIGGER" VALUES('ReactionGT',FALSE,'O:Object,I:Value','Returns true if the reaction of the object specified (on the friendly-hostile scale) to the player was greater than specified. Reaction = 10 + rmodchr + rmodrep (see rmodchr.2da and rmodrep.2da).')
INSERT INTO "TRIGGER" VALUES('ReactionLT',FALSE,'O:Object,I:Value','Returns true if the reaction of the object specified (on the friendly-hostile scale) to the player was lower than specified. Reaction = 10 + rmodchr + rmodrep (see rmodchr.2da and rmodrep.2da).')
INSERT INTO "TRIGGER" VALUES('RealGlobalTimerExpired',FALSE,'S:Name,S:Area','Returns true only if the timer with the specified name of the specified area has been set at least once and has now expired.')
INSERT INTO "TRIGGER" VALUES('RealGlobalTimerNotExpired',FALSE,'S:Name,S:Area','Returns true only if the timer with the specified name of the specified area has been set and has not yet expired.')
INSERT INTO "TRIGGER" VALUES('Reputation',FALSE,'O:Object,I:Reputation','Returns true only if the reputation of the specified object is equal to the second parameter.')
INSERT INTO "TRIGGER" VALUES('ReputationGT',FALSE,'O:Object,I:Reputation','Returns true only if the reputation of the specified object is greater than the second parameter.')
INSERT INTO "TRIGGER" VALUES('ReputationLT',FALSE,'O:Object,I:Reputation','Returns true only if the reputation of the specified object is greater than the second parameter.')
INSERT INTO "TRIGGER" VALUES('Reset',FALSE,'O:Object','Only for trap scripts? Returns true only if this trap or trigger was reset in the last script round by the object specified.')
INSERT INTO "TRIGGER" VALUES('See',FALSE,'O:Object','Returns true only if the active CRE can see the specified object which must not be hidden or invisible.')
INSERT INTO "TRIGGER" VALUES('Specifics',FALSE,'O:Object,I:Specifics','Returns true only if the specifics (as set in the CRE file or by the ChangeSpecifics action) of the specified object is equal to the 2nd parameter.')
INSERT INTO "TRIGGER" VALUES('SpellCast',FALSE,'O:Object,I:Spell','Returns true only if the specified object cast the spell in the 2nd paramater in the last script round.')
INSERT INTO "TRIGGER" VALUES('SpellCastInnate',FALSE,'O:Object,I:Spell','Returns true only if the specified object cast the specified innate spell in the last script round. The spell level must match the spells definition (in spell.ids) for the trigger to return true.')
INSERT INTO "TRIGGER" VALUES('SpellCastOnMe',FALSE,'O:Caster,I:Spell','Returns true only if the specified object cast the specified spell on the active CRE in the last scriptround.')
INSERT INTO "TRIGGER" VALUES('SpellCastPriest',FALSE,'O:Object,I:Spell','Returns true only if the specified object cast the specified priest spell in the last script round.')
INSERT INTO "TRIGGER" VALUES('StateCheck',FALSE,'O:Object,I:State','Returns true only if the specified object is in the state specified.')
INSERT INTO "TRIGGER" VALUES('StealFailed',FALSE,'O:Object','For shopkeepers. Returns true if the specified object failed to steal from the shop in the last script round.')
INSERT INTO "TRIGGER" VALUES('Summoned',FALSE,'O:Object','Returns true only if the active CRE summoned the specified object in the last script round.')
INSERT INTO "TRIGGER" VALUES('TargetUnreachable',FALSE,'O:Object','Returns true only if the object specified cannot be reached by normal walking by the active CRE.')
INSERT INTO "TRIGGER" VALUES('Time',FALSE,'I:Time','Returns true only if the period of day matches the period in the 2nd parameter (taken from Time.ids). Hours are offset by 30 minutes, e.g. Time(1) is true between 00:30 and 01:29.')
INSERT INTO "TRIGGER" VALUES('TimeGT',FALSE,'I:Time','Returns true only if the current time is greater than that specified. Hours are offset by 30 minutes, e.g. TimeGT(1) is true between 01:30 and 02:29.')
INSERT INTO "TRIGGER" VALUES('TimeLT',FALSE,'I:Time','Returns true only if the current time is less than that specified. Hours are offset by 30 minutes, e.g. TimeLT(1) is true between 23:30 and 00:29.')
INSERT INTO "TRIGGER" VALUES('TimeStopCounter',TRUE,'I:Number','Compares the number of ticks left of time stop to Number.')
INSERT INTO "TRIGGER" VALUES('TimeStopCounterGT',TRUE,'I:Number','Compares the number of ticks left of time stop to Number.')
INSERT INTO "TRIGGER" VALUES('TimeStopCounterLT',TRUE,'I:Number','Compares the number of ticks left of time stop to Number.')
INSERT INTO "TRIGGER" VALUES('TimeStopObject',TRUE,'O:Object','Returns true if the the target sprite specified by Object is the caster of time stop (and is exempt from the effect).')
INSERT INTO "TRIGGER" VALUES('TimerExpired',FALSE,'I:ID','Returns true only if the local timer with the specified ID has expired.')
INSERT INTO "TRIGGER" VALUES('TookDamage',FALSE,NULL,'Returns true if some type of damage was caused to the active CRE (and HP were lost) in the last script round.')
INSERT INTO "TRIGGER" VALUES('TrapTriggered',FALSE,'O:Triggerer','Only for trigger regions/traps. Returns true if this trap was triggered by the object specified.')
INSERT INTO "TRIGGER" VALUES('Trigger',FALSE,'I:TriggerNum','Used as a form of script communication, in conjunction with the SendTrigger() action. Seems to have the same range as GiveOrder, and affects only one creature at once. The LastTrigger object does not get set after receiving a trigger.')
INSERT INTO "TRIGGER" VALUES('True',FALSE,NULL,'Always returns true.')
INSERT INTO "TRIGGER" VALUES('TurnedBy',FALSE,'O:Object','Returns true only if the active CRE was turned by the specified priest or paladin.')
INSERT INTO "TRIGGER" VALUES('Unlocked',FALSE,'O:Object','Only for door scripts - returns true only if this door was unlocked by the specified object in the last script round. Appears to be broken.')
INSERT INTO "TRIGGER" VALUES('Unusable',FALSE,'S:ResRef','Returns true only if the specified item is unusable by the active CRE.')
INSERT INTO "TRIGGER" VALUES('WalkedToTrigger',FALSE,'O:Object','Only for trigger regions. Returns true if the specified object has walked to this trigger.')
INSERT INTO "TRIGGER" VALUES('XP',FALSE,'O:Object,I:XP','Returns true if the specified object has experience points equal to the number specified.')
INSERT INTO "TRIGGER" VALUES('XPGT',FALSE,'O:Object,I:XP','Returns true if the specified object has experience points greater than the number specified.')
INSERT INTO "TRIGGER" VALUES('XPLT',FALSE,'O:Object,I:XP','Returns true if the specified object has experience points less than the number specified.')
